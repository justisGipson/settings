    "customizeUI.activityBar": "bottom",
    "customizeUI.font.monospace": "FiraCode-Retina",
    "customizeUI.font.regular": "FiraCode-Retina",
    "customizeUI.fontSizeMap": {
        "13px": "10px",
        "monospace": "9px"
    },
    "customizeUI.moveStatusbar": true,
    "customizeUI.titleBar": "inline",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorStyle": "block-outline",
    "editor.fontFamily": "FiraCode-Retina",
    "editor.fontLigatures": true,
    "editor.fontSize": 11,
    "editor.fontWeight": "500",
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "same",
    "editor.wordWrapColumn": 120,
    "editor.minimap.maxColumn": 200,
    "editor.minimap.renderCharacters": false,
    "editor.smoothScrolling": true,
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "html",
        "typescriptreact"
    ],
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "go.toolsManagement.autoUpdate": true,
    "helium-icon-theme.activeIconPack": "react",
    "helium-icon-theme.hidesExplorerArrows": true,
    "highlight-matching-tag.styles": {
        "opening": {
            "name": {
                "highlight": "purple"
            }
        }
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "html.format.wrapAttributes": "preserve",
    "liveServer.settings.donotShowInfoMsg": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "markdown.extension.toc.updateOnSave": false,
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wrappingIndent": "same",
        "editor.wordWrapColumn": 120,
        "editor.defaultFormatter": "yzhang.markdown-all-in-one",
    },
    "prettier.proseWrap": "always",
    "python.languageServer": "Microsoft",
    "python.showStartPage": false,
    "sync.gist": "fee421ee6e76266fc8643c87d0e59240",
    "telemetry.enableTelemetry": false,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.fontSize": 11,
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "terminal.integrated.lineHeight": 1.1,
    "todohighlight.defaultStyle": {
        "color": "#E0FFFF",
        "backgroundColor": "mediumpurple",
        "overviewRulerColor": "plum",
        "cursor": "pointer",
        "border": "1px solid silver",
        "borderRadius": "2px",
        "isWholeLine": true,
    },
    "todohighlight.isEnable": true,
    "todohighlight.keywords": [
        "DEBUG:",
        "REVIEW:",
        {
            "text": "NOTE:",
            "color": "#4169E1",
            "backgroundColor": "lightgrey",
            "overviewRulerColor": "grey"
        },
        {
            "text": "HACK:",
            "color": "#000",
            "isWholeLine": false,
            "backgroundColor": "green",
        },
        {
            "text": "TODO:",
            "color": "red",
            "border": "1px solid red",
            "borderRadius": "2px", //NOTE: using borderRadius along with `border` or you will see nothing change
            "backgroundColor": "rgba(0,0,0,.2)",
        },
        {
            "text": "XXX:",
            "color": "green",
            "border": "1px solid green",
            "borderRadius": "2px",
            "backgroundColor": "rgba(0,0,0,0.2)",
        },
    ],
    "todo-tree.tree.showScanModeButton": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "todo-tree.filtering.includeHiddenFiles": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.title": "${activeEditorShort}${separator}${dirty}",
    "window.titleBarStyle": "native",
    "workbench.activityBar.visible": true,
    "workbench.colorTheme": "BeardedTheme Surprising Blueberry",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.wrapTabs": true,
    "workbench.productIconTheme": "fluent-icons",
    "workbench.iconTheme": "helium-icon-theme",
    "workbench.startupEditor": "newUntitledFile",
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "yaml.format.printWidth": 100,
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "window.zoomLevel": -2,
}
